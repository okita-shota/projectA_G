CREATE TABLE users (
    user_id INT UNSIGNED AUTO_INCREMENT PRIMARY KEY,
    gender VARCHAR(10),
    birth_date DATE,
    height INT,
    weight INT,
    nickname VARCHAR(50),
    bedtime TIME,
    wakeup_time TIME,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP
);
